{
    "name": "skopenow/oas-lumen",
    "description": "A package for delivering a Lumen REST API from an Open API Schema (OAS) specification.",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/danballance/oas-lumen/issues?state=open",
        "forum": "https://gitter.im/danballance/oas-lumen",
        "source": "https://github.com/danballance/oas-lumen"
    },
    "keywords": [
        "swagger",
        "oai",
        "oas",
        "oas2",
        "oas3",
        "open api",
        "open api initiative",
        "open api specification",
        "lumen",
        "laravel",
        "rest",
        "api"
    ],
    "authors": [
        {
            "name": "Dan Ballance",
            "email": "work@danballance.uk",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "laravel/lumen-framework": "5.8.*",
        "league/fractal": "^0.18.0",
        "Skopenow/oas-tools": "dev-master",
        "symfony/psr-http-message-bridge": "^1.2",
        "zendframework/zend-diactoros": "^2.1",
        "doctrine/common": "^2.10",
        "laravel-doctrine/orm": "^1.4",
        "nette/php-generator": "^3.2",
        "appzcoder/lumen-routes-list": "^1.0",
        "nyholm/psr7": "^1.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpunit/phpunit": "^8.1",
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
        "psr-4": {
            "DanBallance\\OasLumen\\": "./src/OasLumen",
            "DanBallance\\OasLumen\\Tests\\": "./tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanBallance\\OasLumen\\": "./src/OasLumen",
            "DanBallance\\OasLumen\\Tests\\": "./tests"
        }
    },
    "config": {
        "preferred-install": {
            "*": "auto"
        }
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
